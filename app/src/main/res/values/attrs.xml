<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean"/>
        <attr name="ios" format="boolean"/>
        <attr name="leftSwipe" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CenterStrokeTextView">
        <attr name="stroke_color" format="color"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="stroke_bold" format="boolean"/>
        <attr name="font_type" format="enum">
            <enum name="DINMittelschrift_Alternate" value="1"/>
            <enum name="Refrigerator_Deluxe_Heavy" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="AudioView">
        <attr name="linear_width" format="dimension" />
        <attr name="linear_margin" format="dimension" />
        <attr name="linear_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="CircliCornerProgressBar">
        <attr name="radius" format="dimension" />
        <attr name="bgColor" format="color"/>
        <attr name="pgColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="TestView">
        <attr name="tv_stroke_width" format="dimension" />
        <attr name="android:layout_width"/>
        <attr name="android:layout_height"/>
    </declare-styleable>
    
</resources>