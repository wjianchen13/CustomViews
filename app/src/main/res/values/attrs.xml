<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean"/>
        <attr name="ios" format="boolean"/>
        <attr name="leftSwipe" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CenterStrokeTextView">
        <attr name="stroke_color" format="color"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="font_type" format="enum">
            <enum name="DINMittelschrift_Alternate" value="1"/>
            <enum name="Refrigerator_Deluxe_Heavy" value="2"/>
        </attr>
    </declare-styleable>
    
</resources>