<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".viewpager.ViewPagerActivity" />
        <activity android:name=".viewdraghelper.base.BaseActivity" />
        <activity android:name=".viewdraghelper.temp.TempActivity" />
        <activity android:name=".viewdraghelper.ViewDragHelperActivity" />
        <activity android:name=".velocitytracker.VelocitytrackerBaseActivity" />
        <activity android:name=".velocitytracker.VelocitytrackerActivity" />
        
    </application>

</manifest>